        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.ActivityThongTinPKTT"></activity>
        <activity android:name=".Activities.ActivityCTDH" />
        <activity android:name=".Activities.ActivityDSQA" />
        <activity android:name=".Activities.ActivityTinTuc" />
        <activity android:name=".Activities.ActivityCTQA" />
        <activity android:name=".Activities.ActivityThongTinGiaoHang" />
        <activity android:name=".Activities.ActivitySignUp" />

